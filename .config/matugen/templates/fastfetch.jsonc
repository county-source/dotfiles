{
    "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
    "logo": {
        "type": "builtin",
        "height": 15,
        "width": 30,
        "padding": {
            "top": 5,
            "left": 3
        },
        "color": {
            "1": "{{colors.primary.default.hex}}",
            "2": "{{colors.secondary.default.hex}}"
        }
    },
    "modules": [
        "break",
        {
            "type": "custom",
            "format": "\u001b[90m┌──────────────────────Hardware──────────────────────┐"
        },
        {
            "type": "host",
            "key": " PC",
            "keyColor": "{{colors.primary.default.hex}}"
        },
        {
            "type": "cpu",
            "key": "│ ├",
            "keyColor": "{{colors.primary.default.hex}}"
        },
        {
            "type": "gpu",
            "key": "│ ├󰍛",
            "keyColor": "{{colors.primary.default.hex}}"
        },
        {
            "type": "memory",
            "key": "│ ├󰍛",
            "keyColor": "{{colors.primary.default.hex}}"
        },
        {
            "type": "disk",
            "key": "└ └",
            "keyColor": "{{colors.primary.default.hex}}"
        },
        {
            "type": "custom",
            "format": "\u001b[90m└────────────────────────────────────────────────────┘"
        },
        "break",
        {
            "type": "custom",
            "format": "\u001b[90m┌──────────────────────Software──────────────────────┐"
        },
        {
            "type": "os",
            "key": " OS",
            "keyColor": "{{colors.secondary.default.hex}}"
        },
        {
            "type": "kernel",
            "key": "│ ├",
            "keyColor": "{{colors.secondary.default.hex}}"
        },
        {
            "type": "packages",
            "key": "│ ├󰏖",
            "keyColor": "{{colors.secondary.default.hex}}"
        },
        {
            "type": "shell",
            "key": "└ └",
            "keyColor": "{{colors.secondary.default.hex}}"
        },
        "break",
        {
            "type": "wm",
            "key": " WM",
            "keyColor": "{{colors.tertiary.default.hex}}"
        },
        {
            "type": "terminal",
            "key": "└ └",
            "keyColor": "{{colors.tertiary.default.hex}}"
        },
        {
            "type": "custom",
            "format": "\u001b[90m└────────────────────────────────────────────────────┘"
        },
        "break",
        {
            "type": "custom",
            "format": "\u001b[90m┌────────────────────Uptime / Age / DT────────────────────┐"
        },
        {
            "type": "uptime",
            "key": "  Uptime ",
            "keyColor": "{{colors.error.default.hex}}"
        },
        {
            "type": "datetime",
            "key": "  DateTime ",
            "keyColor": "{{colors.error.default.hex}}"
        },
        {
            "type": "custom",
            "format": "\u001b[90m└─────────────────────────────────────────────────────────┘"
        },
        {
            "type": "colors",
            "paddingLeft": 2,
            "symbol": "circle"
        }
    ]
}
